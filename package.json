    "url": "https://github.com/elgervb/php-upload/issues"
  },
  "homepage": "https://github.com/elgervb/php-upload",
  "devDependencies": {
    "gulp": "3.9.0",
    "gulp-if": "2.0.0",
    "gulp-notify": "2.2.0",
    "gulp-phpunit": "0.16.0",
    "lodash": "4.11.1",
    "yargs": "4.6.0"
  }
}
