{
  "name": "php-upload",
  "version": "1.0.0",
  "description": "PHP Upload",
  "main": "test",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "./vendor/phpunit/phpunit/phpunit -c ./phpunit.xml",
    "test:coverage": "gulp test --coverage",
    "watch": "npm test && gulp test:watch",
